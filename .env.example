# Supabase
NEXT_PUBLIC_SUPABASE_URL=YOUR_SUPABASE_URL
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# PostHog
NEXT_PUBLIC_POSTHOG_KEY=YOUR_POSTHOG_KEY
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Sentry (Error Monitoring)
# Get DSN from: https://sentry.io/settings/projects/YOUR_PROJECT/keys/
NEXT_PUBLIC_SENTRY_DSN=

# Sentry Auth Token (for source map uploads during build)
# Create at: https://sentry.io/settings/account/api/auth-tokens/
# Required permissions: project:releases, project:write
SENTRY_AUTH_TOKEN=
